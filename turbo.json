{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/main",
  "pipeline": {
    "migrate": {
      "cache": true
    },
    "dev": {
      "dependsOn": ["migrate"],
      "cache": false
    },
    "build": {
      "outputs": ["dist/**/*", ".next/**/*"],
      "dependsOn": ["^build"]
    }
  }
}