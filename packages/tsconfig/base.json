{
  "compilerOptions": {
    "allowJs": false,
    "strict": false,
    "declaration": true,
    "removeComments": true,
    "allowSyntheticDefaultImports": true,
    "sourceMap": true,
    "incremental": true,
    "skipLibCheck": true,
    "strictNullChecks": false,
    "noImplicitAny": false,
    "strictBindCallApply": false,
    "esModuleInterop": true
  },
  "exclude": [
    "node_modules"
  ]
}